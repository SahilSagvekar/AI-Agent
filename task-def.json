{
  "family": "my-ai-agent-task",
  "executionRoleArn": "arn:aws:iam::043309366307:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "my-ai-agent-container",
      "image": "043309366307.dkr.ecr.ap-southeast-2.amazonaws.com/my-ai-agent:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
